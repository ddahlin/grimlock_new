<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite allow-return-values="true" name="GrimlockAutomation" parallel="tests" thread-count="1" verbose="1">
    <!--for web testing-->

    <parameter name="browser" value="Chrome"/>
    <parameter name="device" value="ZY2256FQ5K"/>
    <parameter name="web_object_to_test" value="jwplayer"/>
    <parameter name="website" value="ChromeMobile"/>
    <parameter name="is_local" value="true"/>
    <parameter name="is_mobile" value="true"/>
    <parameter name="network_sniffer" value="ZY2256FQ5K"/>
    <parameter name="test_name" value="REG3203 - Asset Type: Simple Video  - Mime Type/API Framework: video/mp4  - Device: Android - Browser: Chrome"/>

    <test name="REG3203 Android Chrome" parallel="methods" thread-count="1">
        <groups>
            <run>
                <include name="REG3203"/>
            </run>
        </groups>
        <packages>
            <package name=".*"/>
        </packages>
    </test>
</suite>


