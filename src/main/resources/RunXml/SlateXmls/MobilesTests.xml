    <test name="Local Iphone X Slate Test Case" parallel="methods" thread-count="1">
        <parameter name="os" value="false"/>
        <parameter name="os_version" value="false"/>
        <parameter name="browser" value="safari"/>
        <parameter name="browser_version" value=""/>
        <parameter name="web_object_to_test" value="vast"/>
        <!--for mobile testing-->
        <parameter name="device_udid" value="aaf55afa213e951ef6f646c9ba537d0aac253562"/>
        <parameter name="device_name" value="iPhone X"/>
        <parameter name="device_version" value="12.2"/>
        <parameter name="application_path"
                   value=""/>
        <parameter name="application_name" value="jwplayer"/>
        <parameter name="platform" value="ios"/>
        <parameter name="xcode_org_id" value="PWA73G5RT2"/>
        <!--mandatory for both-->
        <parameter name="is_local" value="true"/>
        <parameter name="test_name" value="xml 2 params test"/>
        <parameter name="reopen_driver_before_each_test" value="false"/>
        <parameter name="ad_type" value="slate"/>
        <parameter name="is_mobile" value="true"/>


        <parameter name="network_sniffer" value="bmp"/>
        <parameter name="network_sniffer_port" value="8082"/>

        <parameter name="browserstack_user_name" value="jeffreysoirefman1"/>
        <parameter name="browserstack_key" value="k4UDGKmXMNWfMfdqWck2"/>


        <groups>
            <run>
                <include name="slate"/>
            </run>
        </groups>
        <packages>
            <package name=".*"/>
        </packages>
    </test>

